function onCreated()
  {
  for ( temp.i = 0; temp.i < playerscount; temp.i ++ )
    {
    showimg( 200 + temp.i, "tutorial_arrowdown.png", this[temp.i].x + 1, this[temp.i].y - 1 );
    changeimgvis( 200 + temp.i, 1 );
  }
  showcharacter();
  this.ani = "idle";
  this.bodyimg = "body5.png";
  this.headimg = "head46.png";
  this.shieldimg = "longtie-shield.png";
  this.colors[0] = "orange"; // Skin
  this.colors[1] = "black"; // Coat
  this.colors[2] = "black"; // Sleeves
  this.colors[3] = "black"; // Shoes
  this.colors[4] = "black"; // Belt
  this.nick = "Arpethix";
  this.ap = 99;
  this.chat = "";
}

function onPlayerTouchsMe() {
triggeraction(0,0,"serverside","-Player/Quests","nv0");
}

function onActionGrab()
  {
   if (clientr.quest.nubevil1 = 0)
     {
  say2("Hey new guy! Welcome to Corinthia" NL
"Being new and all, I'd like to" NL
"welcome you to Nube Village!" NL
"To get you started here, go on" NL
"outside and find the Village Shop." NL
"When you get there, purchase some" NL
"Bombs and Arrows. After that, come" NL
"back and talk to me again.");
clientr.quest.nubevil1 = 1;
}elseif (clientr.quest.nubevil1 = 1)
{
say2("Go get some Bombs and Arrows!");
}elseif (clientr.quest.nubevil1 = 2)
{
say2("Great job! That's the town" NL
"Item Shop. Those Bombs and Arrows" NL
"will come in handy on your quest" NL
"to master your element. Now get" NL
"on out of here, and explore the" NL
"rest of town");
clientr.quest.nubevil1 = 3;
}
}
